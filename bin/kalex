use strict;

use Locale::Messages qw(setlocale LC_MESSAGES);

use Parse::Kalex;

setlocale(LC_MESSAGES, '');
my $parser = Parse::Kalex->newFromArgv(\@ARGV);
$parser->run or exit 1;
$parser->output or exit 2;
